{
    "name": "imatic-it/eduagroup-mangoshop",
	"type": "library",
	"description": "Simple almost event-sourced Nextras ORM based e-shop SDK",
	"require": {
		"php": "~8.1",
		"nette/bootstrap": "~v3.1.4",
        "nette/caching": "^3.3",
        "nette/di": "^3.2",
		"nette/mail": "~v4.0.0",
        "nette/utils": "^4.0",
        "nextras/dbal": "~4.0-dev",
        "nextras/orm": "~4.0-dev",
		"marc-mabe/php-enum": "~3.0",
		"gopay/payments-sdk-php": "~1.3.4",
		"psr/log": "~1.0",
		"mangoweb/exception-responsibility": "dev-master",
		"symfony/console": "~3.4",
		"contributte/console": "^0.2.0"
	},
	"require-dev": {
		"nette/robot-loader": "~v4.0.1",
		"nette/tester": "~2.0",
		"phpstan/phpstan-nette": "~0.9",
		"mockery/mockery": "~1.0",
		"mangoweb/presenter-tester": "@dev",
		"mangoweb/tester-infrastructure": "@dev",
		"nextras/migrations": "~3.1@dev",
		"tracy/tracy": "~2.4",
		"fzaninotto/faker": "dev-master"
	},
	"autoload": {
		"classmap": [
			"apps/",
			"packages/",
			"src/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/phpstan/",
			"tests/src/"
		]
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "Imatic sofrware s.r.o.",
			"email": "info@imatic.cz"
		}
	],
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"classmap-authoritative": true,
		"bin-compat": "full"
	},
	"minimum-stability": "stable"
}
